<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Signup Page</title>

  </head>
  <body>
    <div class="wrapper">
      <div class="title-text">
        <div class="title signup">Signup</div>
      </div>
      <div class="form-container">
        <div class="slide-controls">
          
         
          <label for="user" class="slide user">User</label>
          <label for="business" class="slide business">Business</label>
          <div class="slider-tab"></div>
        </div>
        <div class="form-inner">
          <form
            action="/admin/cdata"
            class="business"
            method="post"
            autocomplete="off"
           
          >
            <div class="field">
              <input
                type="text"
                name="cname"
                id="cname"
                placeholder="CName"
                required
              />
              <span id="namee" class="validation-message"></span>
            </div>
            <div class="field">
              <input
                type="text"
                id="position"
                name="position"
                placeholder="Position"
                required
              />
              <span id="positionn" class="validation-message"></span>
            </div>
            <div class="field">
              <input
                type="date"
                id="sdate"
                name="sdate"
                placeholder="Start Date"
                required
              />
              <span id="sdatee" class="validation-message"></span>
            </div>
            <div class="field">
              <input
                type="date"
                id="edate"
                name="edate"
                placeholder="End Date"
                required
              />
              <span id="edatee" class="validation-message"></span>
            </div>
            <div class="field">
              <input
                type="date"
                id="adate"
                name="adate"
                placeholder="Application Deadline"
                required
              />
              <span id="adate" class="validation-message"></span>
            </div>
            
            <div class="field btn">
              <div class="btn-layer"></div>
              <input type="submit" value="Signup" />
            </div>
            
          </form>
        </div>
      </div>
    </div>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">ID</th>
          <th scope="col">name</th>
          <th scope="col">username</th>
          <th scope="col">email</th>
          <th scope="col">phno</th>
        </tr>
      </thead>
      <tbody>
          <% clist.forEach(companydata => { %>
              <tr>
                  <th scope="row"><%= companydata._id%></th>
                  <td><%= companydata.cname%></td>
                  <td><%= companydata.adate%></td>
                  <td><%= companydata.edate%></td>
                  <td><%= companydata.position%></td>
                  <td><%= companydata.sdate%></td>
                  
              </tr>
          <% }) %>
        
        
      </tbody>
    </table>
    
  </body>
</html>
